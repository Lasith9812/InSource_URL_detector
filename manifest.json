{
  "manifest_version": 2,

  "name": "Python Chrome",
  "description": "This extension runs Python code.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "CurrentURL.html"
  },
  "permissions": [
    "tab",
    "activeTab",
    "contextMenus",
    "https://ajax.googleapis.com/"
  ]
}